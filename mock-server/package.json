{
  "name": "rush-app-mock-dev-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "yarn clean-mock-server && yarn mock-server-ts-compile",
    "clean-mock-server": "rm -rf dist",
    "delete-blob": "npx netlify blobs:delete",
    "mock-server-ts-compile": "npx tsc",
