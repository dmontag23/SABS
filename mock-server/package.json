  "dependencies": {
    "@netlify/blobs": "^7.0.1",
    "@netlify/functions": "^2.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@types/express": "^4.17.21",
    "netlify-cli": "^17.19.4",
    "typescript": "^5.3.3"
  }
}
